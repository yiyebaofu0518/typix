{
	"name": "typix",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:worker": "wrangler dev",
		"build": "tsc -b && cross-env RUNTIME=cloudflare vite build",
		"build:node": "tsc -b && vite build && tsup --minify",
		"analyze": "vite build --mode analyze",
		"lint": "eslint .",
		"preview": "vite preview",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate:dev:worker": "wrangler d1 migrations apply --local typix",
		"db:migrate:worker": "wrangler d1 migrations apply --remote typix",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"deploy": "npm run db:migrate:dev:worker && wrangler deploy",
		"deploy:no-migrate": "wrangler deploy"
	},
	"dependencies": {
		"@hono/node-server": "^1.18.0",
		"@hono/zod-validator": "^0.7.0",
		"@hookform/resolvers": "^5.1.1",
		"@libsql/client": "^0.15.7",
		"@lobehub/icons": "^2.9.0",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-dropdown-menu": "^2.1.15",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-switch": "^1.2.5",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@tailwindcss/vite": "^4.1.7",
		"@tanstack/react-router": "^1.120.10",
		"@tanstack/react-router-devtools": "^1.120.10",
		"@vlcn.io/crsqlite-wasm": "^0.16.0",
		"@vlcn.io/wa-sqlite": "^0.22.0",
		"better-auth": "^1.2.8",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.43.1",
		"drizzle-orm-crsqlite-wasm": "^0.0.7",
		"drizzle-zod": "^0.8.2",
		"hono": "^4.7.10",
		"i18next": "^25.2.1",
		"i18next-browser-languagedetector": "^8.1.0",
		"lucide-react": "^0.511.0",
		"motion": "^12.17.0",
		"nanoid": "^5.1.5",
		"navigator.locks": "^0.8.6",
		"next-themes": "^0.4.6",
		"openai": "^5.9.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-hook-form": "^7.59.0",
		"react-i18next": "^15.5.2",
		"sonner": "^2.0.3",
		"swr": "^2.3.3",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.7",
		"yet-another-react-lightbox": "^3.24.0",
		"zod": "^3.25.67",
		"zustand": "^5.0.5"
